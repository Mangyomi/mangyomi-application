{
    "productName": "Mangyomi Setup",
    "version": "1.0.0",
    "identifier": "com.mangyomi.installer",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../dist"
    },
    "bundle": {
        "active": true,
        "targets": [
            "nsis"
        ],
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "resources": [
            "../resources/app.7z",
            "../resources/installer.blockmap"
        ],
        "windows": {
            "certificateThumbprint": null,
            "digestAlgorithm": "sha256",
            "timestampUrl": ""
        }
    },
    "app": {
        "security": {
            "csp": null
        },
        "windows": [
            {
                "label": "main",
                "title": "Mangyomi Setup",
                "width": 600,
                "height": 450,
                "resizable": false,
                "decorations": false,
                "transparent": true
            }
        ]
    }
}