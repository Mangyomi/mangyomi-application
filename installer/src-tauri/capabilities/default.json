{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Capability for the default window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "core:window:default",
        "core:window:allow-minimize",
        "core:window:allow-maximize",
        "core:window:allow-close",
        "shell:allow-open",
        "dialog:default"
    ]
}